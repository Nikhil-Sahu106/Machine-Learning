   "source": [
    "# Even Fibonacci series"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "aaca7633",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1000\n",
      "798\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "fs0 = 0\n",
    "fs1 = 1\n",
    "i = 2\n",
    "fs = 0\n",
    "for i in range(2,n): \n",
    "    fs3 = fs0 + fs1\n",
    "    fs0 = fs1\n",
    "    fs1 = fs3\n",
    "    if fs3%2==0 and fs3 <= n :\n",
    "        fs += fs3\n",
    "print(fs)"
   ]
